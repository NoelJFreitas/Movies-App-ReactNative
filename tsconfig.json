{
  "extends": "@react-native/typescript-config/tsconfig.json",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@screens/*": ["src/screens/*"],
      "@components/*": ["src/components/*"],
      "@utils/*": ["src/utils/*"],
      "@hooks/*": ["src/hooks/*"],
      "@features/*": ["src/features/*"],
      "@assets/*": ["src/assets/*"],
      "@store/*": ["src/store/*"],
      "@src/*": ["src/*"]
    }
  }
}
